{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$ref": "#/$defs/Project",
    "$defs": {
        "Project": {
            "properties": {
                "log_file": {
                    "type": "string"
                },
                "users": {
                    "items": {
                        "$ref": "#/$defs/User"
                    },
                    "type": "array"
                },
                "auth_users": {
                    "items": {
                        "$ref": "#/$defs/AuthUser"
                    },
                    "type": "array"
                },
                "teams": {
                    "items": {
                        "$ref": "#/$defs/Team"
                    },
                    "type": "array"
                },
                "master_dbs": {
                    "items": {
                        "$ref": "#/$defs/MasterDB"
                    },
                    "type": "array"
                },
                "foreigns": {
                    "items": {
                        "$ref": "#/$defs/Foreign"
                    },
                    "type": "array"
                },
                "extract_dbs": {
                    "items": {
                        "$ref": "#/$defs/ExtractDB"
                    },
                    "type": "array"
                },
                "working_extract_dbs": {
                    "items": {
                        "$ref": "#/$defs/WorkingExtractDB"
                    },
                    "type": "array"
                },
                "dynamic_dbsets": {
                    "items": {
                        "$ref": "#/$defs/DynamicDBSet"
                    },
                    "type": "array"
                },
                "dbsets": {
                    "items": {
                        "$ref": "#/$defs/StaticDBSet"
                    },
                    "type": "array"
                },
                "mdbs": {
                    "items": {
                        "$ref": "#/$defs/MDB"
                    },
                    "type": "array"
                },
                "ttfonts": {
                    "items": {
                        "$ref": "#/$defs/TTFont"
                    },
                    "type": "array"
                },
                "post_commands": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": false,
            "type": "object"
        },
        "AuthUser": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "default_user": {
                    "type": "string"
                },
                "users": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "name"
            ]
        },
        "CopyDB": {
            "properties": {
                "from": {
                    "type": "string"
                },
                "to": {
                    "type": "string"
                },
                "area_number": {
                    "type": "integer"
                },
                "file_number": {
                    "type": "integer"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "from"
            ]
        },
        "DynamicDBSet": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "criteria": {
                    "type": "string"
                },
                "order_by": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "name",
                "criteria",
                "order_by"
            ]
        },
        "Element": {
            "properties": {
                "type": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "type",
                "name"
            ]
        },
        "ExtractDB": {
            "properties": {
                "owner": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "variant": {
                    "type": "boolean"
                },
                "session": {
                    "type": "integer"
                },
                "area_number": {
                    "type": "integer"
                },
                "explicit_claim": {
                    "type": "boolean"
                },
                "extract_number": {
                    "type": "integer"
                },
                "description": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "owner",
                "name"
            ]
        },
        "Foreign": {
            "properties": {
                "project": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "copy_dbs": {
                    "items": {
                        "$ref": "#/$defs/CopyDB"
                    },
                    "type": "array"
                },
                "include_dbs": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "project"
            ]
        },
        "MDB": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "current_dbs": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "deferred_dbs": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "name"
            ]
        },
        "MasterDB": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "ref_only": {
                    "type": "boolean"
                },
                "db_number": {
                    "type": "integer"
                },
                "area_number": {
                    "type": "integer"
                },
                "explicit_claim": {
                    "type": "boolean"
                },
                "protected": {
                    "type": "boolean"
                },
                "extract_number": {
                    "type": "integer"
                },
                "file_number": {
                    "type": "integer"
                },
                "description": {
                    "type": "string"
                },
                "create_element": {
                    "$ref": "#/$defs/Element"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "name",
                "type"
            ]
        },
        "StaticDBSet": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "dbs": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "name"
            ]
        },
        "TTFont": {
            "properties": {
                "index": {
                    "type": "integer"
                },
                "face_name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "index",
                "face_name"
            ]
        },
        "Team": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "members": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "description": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "name"
            ]
        },
        "User": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "free": {
                    "type": "boolean"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "name",
                "password"
            ]
        },
        "WorkingExtractDB": {
            "properties": {
                "owner": {
                    "type": "string"
                },
                "users": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "variant": {
                    "type": "boolean"
                },
                "area_number": {
                    "type": "integer"
                },
                "explicit_claim": {
                    "type": "boolean"
                },
                "description": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "owner",
                "users"
            ]
        }
    }
}